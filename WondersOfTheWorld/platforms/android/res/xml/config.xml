<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ciandt.wondersoftheworld" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Alert">
        <param name="android-package" value="com.ciandt.wondersoftheworld.plugins.AlertPlugin" />
    </feature>
    <feature name="FlowManager">
        <param name="android-package" value="com.ciandt.wondersoftheworld.plugins.FlowManagerPlugin" />
    </feature>
    <feature name="Storage">
        <param name="android-package" value="com.ciandt.wondersoftheworld.plugins.StoragePlugin" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <allow-intent href="market:*" />
    <name>WondersOfTheWorld</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="view/login.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
